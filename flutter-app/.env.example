# Firebase Configuration
# Copy this file to .env and fill in your actual Firebase credentials

# Firebase Project Info
FIREBASE_PROJECT_NUMBER=YOUR_PROJECT_NUMBER
FIREBASE_PROJECT_ID=YOUR_PROJECT_ID
FIREBASE_STORAGE_BUCKET=YOUR_STORAGE_BUCKET

# Firebase Client Info
FIREBASE_MOBILE_SDK_APP_ID=YOUR_MOBILE_SDK_APP_ID
FIREBASE_PACKAGE_NAME=com.example.security_app

# Firebase OAuth Client
FIREBASE_OAUTH_CLIENT_ID=YOUR_OAUTH_CLIENT_ID
FIREBASE_CERTIFICATE_HASH=YOUR_CERTIFICATE_HASH

# Firebase API Key
FIREBASE_API_KEY=YOUR_API_KEY

# Backend API Configuration
BACKEND_API_URL=http://localhost:8000
BACKEND_API_TIMEOUT=30000

# App Configuration
APP_DEBUG_MODE=false
APP_VERSION=1.0.0
